{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Console",
        "Args": {
          "path": "Logs/logs.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "ParanusmusConnectionString": "Server=(localdb)\\MSSQLLocalDB;Database=ParanusmusDb; Trusted_Connection=True;"
  },
  "AllowedHosts": "*"
}
